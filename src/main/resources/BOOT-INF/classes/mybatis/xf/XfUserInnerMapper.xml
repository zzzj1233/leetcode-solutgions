<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.project.xf.mapper.XfUserInnerMapper">
    
    <resultMap type="XfUserInner" id="XfUserInnerResult">
        <result property="id"    column="id"    />
    </resultMap>

    <sql id="selectXfUserInnerVo">
        select id from xf_user_inner
    </sql>

    <select id="selectXfUserInnerList" parameterType="XfUserInner" resultMap="XfUserInnerResult">
        <include refid="selectXfUserInnerVo"/>
        <where>  
        </where>
        ORDER  BY  id DESC
    </select>
    
    <select id="selectXfUserInnerById" parameterType="Long" resultMap="XfUserInnerResult">
        <include refid="selectXfUserInnerVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertXfUserInner" parameterType="XfUserInner" useGeneratedKeys="true" keyProperty="id">
        insert into xf_user_inner
        <trim prefix="(" suffix=")" suffixOverrides=",">
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
         </trim>
    </insert>

    <update id="updateXfUserInner" parameterType="XfUserInner">
        update xf_user_inner
        <trim prefix="SET" suffixOverrides=",">
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteXfUserInnerById" parameterType="Long">
        delete from xf_user_inner where id = #{id}
    </delete>

    <delete id="deleteXfUserInnerByIds" parameterType="String">
        delete from xf_user_inner where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
    
</mapper>